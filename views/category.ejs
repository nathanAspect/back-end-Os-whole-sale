<div class="table-container">
   <table>
     <thead>
       <tr class="table-header">
         <th>Category</th>
         <th>Owner</th>
         <th>Visit</th>
         <th>Total Visit</th>
       </tr>
     </thead>
     <tbody>




      <% for(let i=0; i<list.length; i++){ %>
        <tr class="tableRow">
          <td> <%= list[i].name %> </td>
          <td> <%= list[i].Admin %> </td>
          <td> <%= list[i].Visit %> </td>
          <td> <%= list[i].totalVisit %> </td>
        </tr class="tableRow">
      <% } %>






