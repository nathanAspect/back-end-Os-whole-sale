<%- include('header.ejs') %>




<body>
   <div class="sideMenu">
      <div class="logo">
         <img src="/resources/logo.png" alt="logo">
         <p>OS wholesale</p>
      </div>
      <div class="menu">

         <div class="menuOption" onclick="window.location.href = '/dashboard'">
            <img src="/resources/dashboard.png" alt="dashboard icon">
            <p>Dashboard</p>
         </div>

         <div class="menuOption" onclick="window.location.href = '/dashboard/categories'">
            <img src="/resources/category.png" alt="category icon">
            <p>Categories</p>
         </div>

         <div class="menuOption" onclick="window.location.href = '/dashboard/products'">
            <img src="/resources/product.png" alt="product icon">
            <p>Products</p>
         </div>

         <div class="menuOption" onclick="window.location.href = '/dashboard/account'">
            <img src="/resources/user.png" alt="user icon">
            <p>Account</p>
         </div>

         <div class="menuOption" id="ManageAll" onclick="window.location.href = '/manage'">
            <img src="/resources/owner.png" alt="manager icon">
            <p>MANAGE ALL</p>
         </div>
         <style>
            #ManageAll{
               display: <%= displayValue %>;
            }
         </style>
         
      </div>
   </div>
   <div class="contentHolder">
      <div class="contentNav">
         <div class="searchBar">
            <input type="text" placeholder="Search here">
            <button><img src="/resources/search.png" alt="search icon"></button>
         </div>

         <div class="profile">
            <img src="/resources/user.png" alt="user icon">
            <div class="profilePop">
               <p> <%= userName %> </p>
               <a href="/logout" class="logout">Log out!</a>
            </div>
         </div>
         
      </div>

      <div class="detail">
         

         <%- include(presentContent) %> 

      </div>









         <!-- <div class="footer">
            <p>2024 Â© Influence - Designed by nathanantenehpersonal@gmail.com</p>
         </div> -->
      </div>
      <script src="/script/dash.js"></script>
      </body>
      </html>